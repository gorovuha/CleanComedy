  "@type": "sc:Dataset",
  "name": "gpt-3",
  "description": "This dataset contains English jokes filtered from duplicates and insulting instances.",
  "conformsTo": "http://mlcommons.org/croissant/1.0",
  "citeAs": "CleanComedy",
  "license": "CC BY-SA 4.0",
  "url": "https://anonymous.4open.science/r/CleanComedy-6DCA",
  "distribution": [
    {
      "@type": "cr:FileObject",
      "@id": "github-repository",
      "name": "github-repository",
      "description": "CleanComedy",
      "contentUrl": "https://anonymous.4open.science/r/CleanComedy-6DCA/clean_comedy_en.json",
      "encodingFormat": "git+https",
      "sha256": "main"
    },
    {
      "@type": "cr:FileSet",
      "@id": "json-files",
      "name": "json-files",
      "description": "JSON files are hosted on the GitHub repository.",
      "containedIn": {
        "@id": "github-repository"
      },
      "encodingFormat": "application/json",
      "includes": "data/*.jsonl"
    }
  ],
  "recordSet": [
    {
      "@type": "cr:RecordSet",
      "@id": "json",
      "name": "json",
      "field": [
        {
          "@type": "cr:Field",
          "@id": "json/context",
          "name": "full_joke",
          "description": "English raw joke without preprocessing.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "json-files"
            },
            "extract": {
              "column": "full_joke"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "json/set-up",
          "name": "set-up",
          "description": "The set-up of the full joke",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "json-files"
            },
            "extract": {
              "column": "set-up"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "json/punchline",
          "name": "punchline",
          "description": "The punchline of the full joke",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "json-files"
            },
            "extract": {
              "column": "punchline"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "json/humour_label",
          "name": "humour_label",
          "description": "The automatically annotated humour label",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "json-files"
            },
            "extract": {
              "column": "humour_label"
            }
          }
        }
      ]
    }
  ]
